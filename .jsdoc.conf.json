{
  "plugins": ["plugins/markdown", "jsdoc-plugin-intersection"],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "destination": "./docs",
    "template": "node_modules/clean-jsdoc-theme",
    "readme": "./docs/static/home.md",
    "theme_opts": {
      "default_theme": "light",
      "display-module-header": true,
      "title": "MasohiSerialport API",
      "homepageTitle": "MasohiSerialport API",
      "sections": ["Classes", "Events", "Modules", "Global"],
      "menu": [{
        "title": "NPM",
        "link": "https://www.npmjs.com/package/masohi-serialport"
      }, {
        "title": "Github",
        "link": "https://github.com/ardhi/masohi-serialport"
      }, {
        "title": "Masohi",
        "link": "https://masohi.bajo.app/"
      }, {
        "title": "Bajo",
        "link": "https://bajo.app/"
      }]
    }
  },
  "source": {
    "include": ["."],
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": ["node_modules", "docs", "test"]
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}